---
metadata:
  file_extension: ".scss"
---

// Background Colors
.bg-color-4 {
    background-color: $color-4!important;
}

.bg-color-6 {
    background-color: $color-6!important;
}

// Text Colors
.color-1-text {
    color: $color-1;
}

.color-2-text {
    color: $color-2;
}

.color-3-text {
    color: $color-3!important;
}

.color-4-text {
    color: $color-4;
}

.color-5-text {
    color: $color-5;
}

.color-2-text-all {
    * {
        color: $color-2;
    }
}

.color-1-text-all {
    * {
        color: $color-1;
    }
}